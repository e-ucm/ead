{
    "class": "modelentity",
    "components": [{
        "class": "image",
        "uri": "images/map.png"
    }, {
        "class": "cameras",
        "cameras": [{
            "id": "defaultCamera",
            "x": 0,
            "y": 0,
            "width": 1066,
            "height": 600,
            "rotation": 0
        }, {
            "id": "cameraAlien",
            "x": 51,
            "y": 238,
            "width": 251,
            "height": 141,
            "rotation": 0
        }, {
            "id": "cameraChameleon",
            "x": 610,
            "y": 265,
            "width": 251,
            "height": 141,
            "rotation": 15
        }]
    }],
    "children": [
        // Tree
        {
            "x": 650,
            "y": 0,
            "components": [{
                "class": "image",
                "uri": "images/tree.png"
            }]
        },
        // Chameleon
        {
            "x": 680,
            "y": 280,
            "components": [{
                "class": "image",
                "uri": "images/chameleon.png"
            }, {
                "class": "tags",
                "tags": ["chameleon"]
            }, {
                "class": "behavior",
                "event": {
                    "class": "key",
                    "keycode": 19,
                    "shift": false,
                    "alt": false,
                    "ctrl": false
                },
                "effects": [{
                    "class": "addcomponent",
                    "component": {
                        class: "movetween",
                        "y": 10,
                        "relative": true,
                        "duration": 0,
                        "yoyo": false
                    }
                }]
            }, {
                "class": "behavior",
                "event": {
                    "class": "key",

                    "keycode": 20,
                    "shift": false,
                    "alt": false,
                    "ctrl": false
                },
                "effects": [{
                    "class": "addcomponent",
                    "component": {
                        class: "movetween",
                        "y": -10,
                        "relative": true,
                        "duration": 0,
                        "yoyo": false
                    }
                }]
            }]
        },
        // Bee
        {
            "y": 500,
            "x": 100,
            "scaleX": -1,
            "originX": 20,
            "components": [{
                    "class": "frames",
                    "sequence": "linear",
                    "frames": [{
                        "renderer": {
                            "class": "image",
                            "uri": "images/bee.png",
                            "collider": [{
                                "points": [1, 42, 13, 48, 20,
                                    36, 30, 34, 41, 47, 48,
                                    47, 55, 40, 56, 31, 43,
                                    26, 47, 17, 45, 3, 41,
                                    0, 32, 0, 27, 4, 26, 12,
                                    20, 9, 11, 10, 10, 7,
                                    4, 9, 3, 27, 6, 32, 2,
                                    35
                                ]
                            }]
                        },
                        "time": "0.1"
                    }, {
                        "renderer": {
                            "class": "image",
                            "uri": "images/bee_fly.png",
                            "collider": [{
                                "points": [1, 33, 6, 42, 32, 34, 57, 40,
                                    61, 26, 58, 20, 50, 21, 49, 3, 45,
                                    0, 36, 0, 31, 4, 30, 12, 24, 9, 15,
                                    10, 14, 7, 8, 9, 7, 26, 0, 29
                                ]
                            }]
                        },
                        "time": "0.1"
                    }]
                }, {
                    "class": "movetween",
                    "y": 10,
                    "relative": true,
                    "duration": 0.25,
                    "yoyo": true,
                    "repeat": -1,
                    "easeEquation": "linear"
                }, {
                    "class": "movetween",
                    "x": 800,
                    "relative": true,
                    "duration": 5,
                    "yoyo": true,
                    "repeat": -1,
                    "easeEquation": "quad",
                    "easeType": "inout"
                }, {
                    "class": "scaletween",
                    "delay": 4.5,
                    "scaleX": 2,
                    "relative": true,
                    "duration": 0.5,
                    "repeatDelay": 4.5,
                    "repeat": -1,
                    "yoyo": true
                }, {
                    "class": "rotatetween",
                    "rotation": 45,
                    "relative": true,
                    "duration": 1.68,
                    "yoyo": true,
                    "repeat": -1

                }, {
                    "class": "visibility",
                    "condition": "(eq $showbee btrue)"
                }, {
                    "class": "behavior",
                    "event": {
                        "class": "touch"
                    },
                    "effects": [{
                        "class": "endgame"
                    }]
                }

            ]
        },
        // Alien
        {
            "y": 200,
            "components": [{
                "class": "pathboundary",
                "boundaryPolygons": [{
                    "points": [0, 0, 800, 0, 800, 400, 0, 400]
                }],
                "viewSquare": {
                    "points": [0, 0, 100, 0, 90, 100, 10, 100]
                },
                "speed": 100,
            }, {
                "class": "refrenderer",
                "uri": "anim/alien.json"
            }, {
                "class": "velocity",
                "x": 50,
                "y": 1
            }, {
                "class": "behavior",
                "event": {
                    "class": "touch"
                },
                "effects": [{
                    "class": "goto",
                    "x": "700",
                    "y": "290"
                }]
            }, {
                "class": "fieldtween",
                "component": "velocity",
                "field": "x",
                "target": 0,
                "duration": 5,
                "yoyo": false,
                "repeat": 0
            }, {
                "class": "alphatween",
                "alpha": 0,
                "relative": false,
                "duration": 2,
                "yoyo": true,
                "repeat": -1,
                "easeEquation": "linear",
                "easeType": "inout"

            }]
        }
    ]
}